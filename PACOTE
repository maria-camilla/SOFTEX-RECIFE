package br.com.arquivomorto.pacote;
public class CadastroArquivo {
    string nome;
    /**
     *
     */
    int codigo;
    public CadastroArquivo(string nome, int codigo) {
        this.nome = nome;
        this.codigo = codigo;
    }
    public int getCodigo() {
        return codigo;
    }
    public void setCodigo(int codigo) {
        this.codigo = codigo;
    }
public class CadastroArquivo {
    Arquivo novoArquivo = new Arquivo();
}
}

package br.com.consultadocumentos.pacote;
public class NovoDocumento {
    string nome;
    /**
     *
     */
    string assunto;
    public NovoDocumento(string nome, string assunto) {
        this.nome = nome;
        this.assunto = assunto;
    }
    public string getAssunto() {
        return assunto;
    }
    public void setAssunto(string assunto) {
        this.assunto = assunto;
    }
    public class NovoDocumento {
        Documento novoDocumento = new Documento();
    }
    }
